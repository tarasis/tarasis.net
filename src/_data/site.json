  "highlighter": "rouge",
  "lsi": false,
  "excerpt_separator": "<!--more-->",
  "incremental": false,
  "kramdown": {
    "input": "GFM",
    "hard_wrap": false,
    "auto_ids": true,
    "footnote_nr": 1,
    "entity_output": "as_char",
    "toc_levels": "1..6",
    "smart_quotes": "lsquo,rsquo,ldquo,rdquo",
    "enable_coderay": false
  },
  "sass": {
    "sass_dir": "_sass",
    "style": "compressed"
